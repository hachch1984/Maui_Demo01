<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="FrontEndMovile.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:FrontEndMovile"
    xmlns:views="clr-namespace:FrontEndMovile.View"
    Shell.FlyoutBehavior="Flyout"
    Title="FrontEndMovile">

    <!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />-->

    <TabBar>
        <Tab Title="Inicio" Icon="home.png">
            <ShellContent ContentTemplate="{DataTemplate local:MainPage}" />
        </Tab>
        <Tab Title="Notificaciones" Icon="notifications.png">
            <ShellContent ContentTemplate="{DataTemplate views:NotificationPage}" />
        </Tab>
    </TabBar>

</Shell>
